  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 5
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": "string",
      "minLength": 5
    }
  },
  "required": [
    "username",
    "email",
    "password"
  ]
}